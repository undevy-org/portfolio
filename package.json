{
  "name": "portfolio-project",
  "version": "6.0.4",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:utils": "jest src/app/utils --coverage",
    "test:components": "jest src/app/components/ui --coverage",
    "test:no-coverage": "jest --coverage=false",
    "test:coverage": "c8 --reporter=text --reporter=html npm test",
    "test:coverage:utils": "c8 --reporter=text --include='src/app/utils/**' npm test -- src/app/utils",
    "test:simple-coverage": "jest --coverage --coverageDirectory=coverage --collectCoverageFrom='src/app/utils/{formatters,session}.js' --coveragePathIgnorePatterns='/node_modules/|test\\.js$'",
    "test:ci": "jest --config=jest.config.ci.mjs --watchAll=false",
    "test:ci-fast": "CI_SKIP_INTEGRATION=true jest --config=jest.config.ci.mjs --watchAll=false",
    "test:integration": "jest src/app/integration.test.js",
    "workflow:check": "./scripts/monitor-workflow.sh",
    "release": "standard-version",
    "storybook": "storybook dev -p 6006",
    "storybook:tailwind": "concurrently \"npm:tailwind:watch\" \"npm:storybook\"",
    "tailwind:watch": "tailwindcss -i src/app/globals.css -o .storybook/storybook-tailwind.css --content \"src/**/*\" --watch",
    "tailwind:compile": "tailwindcss -i src/app/globals.css -o .storybook/storybook-tailwind.css --content \"src/**/*\"",
    "build-storybook": "tailwindcss -i src/app/globals.css -o .storybook/storybook-tailwind.css --content \"src/**/*\" && storybook build",
    "test-storybook": "test-storybook",
    "coverage-storybook": "test-storybook --coverage",
    "verify-tailwind": "tailwindcss -i src/app/globals.css -o .storybook/storybook-tailwind.css --content \"src/**/*\" --verbosity"
  },
  "dependencies": {
    "@reown/appkit": "^1.7.18",
    "@reown/appkit-adapter-wagmi": "^1.7.18",
    "@tanstack/react-query": "^5.84.1",
    "lucide-react": "^0.528.0",
    "next": "15.4.7",
    "qr": "^0.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "viem": "^2.33.2",
    "wagmi": "^2.16.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@chromatic-com/storybook": "3.2.7",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/experimental-addon-test": "8.6.14",
    "@storybook/nextjs": "8.6.14",
    "@storybook/react-webpack5": "8.6.14",
    "@storybook/test": "8.6.14",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "^10.4.21",
    "babel-loader": "10.0.0",
    "c8": "^10.1.3",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "eslint-plugin-storybook": "9.0.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-silent-reporter": "^0.6.0",
    "pino-pretty": "^13.1.1",
    "playwright": "1.55.0",
    "postcss": "^8.5.6",
    "standard-version": "^9.5.0",
    "storybook": "8.6.14",
    "tailwindcss": "^3.4.17",
    "vitest": "3.2.4",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "overrides": {
    "glob": "^10.4.5",
    "inflight": "npm:lru-cache@10.0.0",
    "zustand": "5.0.0"
  },
  "resolutions": {
    "zustand": "5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
